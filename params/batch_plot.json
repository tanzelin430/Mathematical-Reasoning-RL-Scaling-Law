{
  "runs": [
    {
      "comment": "Base model - fit N curve with C and E, then plot",
      "data_source": "exp2-base",
      "plot_curve_column": "N",
      "plot_x_columns": [
        "C",
        "E"
      ],
      "eval": "holdout_score",
      "plot_metrics": [
        "ErrRate"
      ],
      "warmup_clip_factor_raw": 0.1,
      "fit": true,
      "fit_curve_column": "N",
      "fit_x_column": "C",
      "fit_metrics": [
        "ErrRate"
      ],
      "plot_x_scale": "log",
      "plot_y_scale": "log",
      "output_prefix": "fit_base_"
    },
    {
      "comment": "Instruct model - same configuration for comparison",
      "data_source": "exp2-instruct",
      "plot_curve_column": "N",
      "plot_x_columns": [
        "C",
        "E"
      ],
      "eval": "holdout_score",
      "plot_metrics": [
        "ErrRate"
      ],
      "warmup_clip_factor_raw": 0.02,
      "fit": true,
      "fit_curve_column": "N",
      "fit_x_column": "C",
      "fit_metrics": [
        "ErrRate"
      ],
      "plot_x_scale": "log",
      "plot_y_scale": "log",
      "plot_curve_mask": [
        5e8,
        1.5e9,
        3e9,
        7e9,
        14e9
      ],
      "highlight_curves": [
        7e9
      ],
      "output_prefix": "instruct_N_fit_"
    },
    {
      "comment": "Base model - Tau curves with multiple metrics",
      "data_source": "exp2-base",
      "plot_curve_column": "Tau",
      "plot_x_columns": [
        "E"
      ],
      "eval": "holdout_score",
      "plot_metrics": [
        "ErrRate",
        "R"
      ],
      "warmup_clip_factor_raw": 0.02,
      "fit": false,
      "plot_x_scale": "log",
      "plot_y_scale": "log",
      "plot_curve_mask": [
        1,
        2,
        5,
        10,
        20,
        50,
        100
      ],
      "highlight_curves": [
        1,
        20
      ],
      "output_prefix": "base_Tau_nofit_"
    },
    {
      "comment": "Advanced - fit Tau curves and save model",
      "data_source": "exp2-instruct",
      "plot_curve_column": "Tau",
      "plot_x_columns": [
        "E",
        "C"
      ],
      "eval": "holdout_score",
      "plot_metrics": [
        "ErrRate"
      ],
      "warmup_clip_factor_raw": 0.02,
      "fit": true,
      "fit_curve_column": "Tau",
      "fit_x_column": "E",
      "fit_metrics": [
        "ErrRate"
      ],
      "fit_save_path": "outputs/model_exp2-instruct_Tau_E_batch.json",
      "plot_x_scale": "log",
      "plot_y_scale": "log",
      "plot_curve_mask": [
        1,
        2,
        5,
        20,
        25,
        100
      ],
      "highlight_curves": [
        1
      ],
      "output_prefix": "instruct_Tau_fit_save_"
    }
  ]
}