{
  "runs": [
    {
      "comment": "Instruct model - fit N curve with C and E, then plot",
      "data_source": "instruct",
      "plot_curve_column": "N",
      "plot_x_columns": [
        "C",
        "E"
      ],
      "eval": "holdout_score",
      "plot_metrics": [
        "ErrRate"
      ],
      "warmup_clip_factor_raw": 0.1,
      "fit": true,
      "fit_curve_column": "N",
      "fit_x_column": "C",
      "fit_metrics": [
        "ErrRate"
      ],
      "plot_x_scale": "log",
      "plot_y_scale": "log",
      "line_alpha": 1,
      "line_width": 2.0,
      "scatter_alpha": 1,
      "scatter_size": 10,
      "scatter_marker": "s",
      "output_prefix": "fit_instruct_"
    }
  ]
}