{
  "runs": [
    {
      "comment": "figure 1a",
      "data_source": "base",
      "plot_curve_column": "N",
      "plot_x_columns": [
        "C_raw"
      ],
      "eval": "holdout_score",
      "plot_metrics": [
        "ErrRate"
      ],
      "warmup_clip_factor_raw": 0.1,
      "fit": false,
      "fit_curve_column": "N",
      "fit_x_column": "C_raw",
      "fit_metrics": [
        "ErrRate"
      ],
      "add_smooth": true,
      "smooth_monotonic": true,
      "s_factor": 1,
      "k_spline": 3,
      "rolling_window": 200,
      "min_se": 1e-3,
      "x_inv_weight_power": 0.2,
      "plot_x_scale": "log",
      "plot_y_scale": "log",
      "y_tick_spacing": 0.1,
      "y_tick_format": "auto",
      "y_grid_spacing": 0.1,
      "line_alpha": 1,
      "line_width": 2.0,
      "plot_use_legend": true,
      "plot_title": "Smoothed L(N, C) on Base model",
      "scatter_alpha": 0.3,
      "scatter_size": 10,
      "scatter_marker": "o",
      "output_prefix": "smooth_base_"
    }
  ]
}